	<queryString>
		<![CDATA[SELECT ca.idCabo,
       ca.Descricao,
       b.idbobina,
       b.QtdAtual + ifnull( (
                                SELECT sum(b.QtdAtual)
                                  FROM Cabo c
                                       INNER JOIN
                                       Bobina b ON ca.idCabo = b.id_Cabo AND
                                                   b.Ativo = 'n'
                                 GROUP BY idCabo
                            ), 0) AS [QTD Total bobinas],
       ifnull( (
                   SELECT sum(Quantidade)
                     FROM Ponta p
                          INNER JOIN
                          Cabo ON p.id_Cabo = ca.idCabo
                    GROUP BY idCabo
               ), 0) AS [QTD Total em pontas ],
       (b.QtdAtual + ifnull( (
                                 SELECT sum(b.QtdAtual)
                                   FROM Cabo c
                                        INNER JOIN
                                        Bobina b ON ca.idCabo = b.id_Cabo AND
                                                    b.Ativo = 'n'
                                  GROUP BY idCabo
                             ), 0) ) + (ifnull( (
                                                    SELECT sum(Quantidade)
                                                      FROM Ponta p
                                                           INNER JOIN
                                                           Cabo ON p.id_Cabo = ca.idCabo
                                                     GROUP BY idCabo
                                                ), 0) ) AS [QTD Total]
  FROM Cabo ca
       INNER JOIN
       Bobina b ON ca.idCabo = b.id_Cabo AND
                   b.Ativo = 's'
 ORDER BY 2,
          4,
          5;]]>
	</queryString>
