	<queryString>
		<![CDATA[SELECT c.idCabo,c.Descricao,c.PossuiBobina,
     b.idBobina,b.QtdAtual,b.Quantidade,
     case Ativo
     when  's' then 'Ativo'
     else  'Inativo'
     end
